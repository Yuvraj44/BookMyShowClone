  "components": {
    "schemas": {
      "LogInDTO": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Movie": {
        "required": [
          "description",
          "duration",
          "genre",
          "imageUrl",
          "title"
        ],
        "type": "object",
        "properties": {
          "movieId": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "maxLength": 255,
            "minLength": 1,
            "type": "string"
          },
          "imageUrl": {
            "minLength": 1,
            "type": "string",
            "format": "uri"
          },
          "genre": {
            "minLength": 1,
            "type": "string"
          },
          "duration": {
            "minLength": 1,
            "pattern": "^\\d+\\smin$",
            "type": "string"
          },
          "description": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ShowsDTO": {
        "type": "object",
        "properties": {
          "movieId": {
            "type": "integer",
            "format": "int32"
          },
          "cinemaHall": {
            "type": "string",
            "nullable": true
          },
          "date": {
            "type": "string",
            "nullable": true
          },
          "timing": {
            "type": "string",
            "nullable": true
          },
          "availableSeats": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "TicketDTO": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "showId": {
            "type": "integer",
            "format": "int32"
          },
          "tickets": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Users": {
        "required": [
          "email",
          "isAdmin",
          "name",
          "password"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 255,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "picture": {
            "type": "string",
            "nullable": true
          },
          "isAdmin": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
